{
  "name": "church-wallet-server",
  "version": "1.0.0",
  "description": "Church Wallet Keeping System - Backend API",
  "main": "dist/server.js",
  "scripts": {
    "start": "node dist/server.js",
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "build": "tsc",
    "type-check": "tsc --noEmit",
    "seed": "ts-node-dev --transpile-only src/seed.ts",
    "seed:new": "ts-node-dev --transpile-only src/seedNew.ts",
    "seed:complete": "ts-node-dev --transpile-only src/seedComplete.ts",
    "seed:christmas": "ts-node-dev --transpile-only src/seedChristmas.ts",
    "db:clear": "ts-node-dev --transpile-only src/clearDatabase.ts",
    "db:reset": "ts-node-dev --transpile-only src/seedComplete.ts",
    "migrate:hierarchical": "ts-node-dev --transpile-only src/scripts/migrate-hierarchical-numbers.ts",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "church",
    "wallet",
    "finance",
    "mongodb",
    "express",
    "typescript"
  ],
  "author": "Vishnu",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.6.0",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-validator": "^7.0.1",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.0.0",
    "node-cron": "^4.2.1",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.0.0",
    "@types/node-cron": "^3.0.11",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.0"
  }
}
